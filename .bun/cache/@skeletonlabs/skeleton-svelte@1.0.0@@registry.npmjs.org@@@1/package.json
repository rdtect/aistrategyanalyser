{
  "name": "@skeletonlabs/skeleton-svelte",
  "version": "1.0.0",
  "description": "The Svelte package for Skeleton.",
  "author": "endigo9740 <chris@skeletonlabs.dev>",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*"
  ],
  "dependencies": {
    "@zag-js/accordion": "^1.3.1",
    "@zag-js/avatar": "^1.3.1",
    "@zag-js/combobox": "^1.3.1",
    "@zag-js/dialog": "^1.3.1",
    "@zag-js/file-upload": "^1.3.1",
    "@zag-js/pagination": "^1.3.1",
    "@zag-js/popover": "^1.3.1",
    "@zag-js/progress": "^1.3.1",
    "@zag-js/radio-group": "^1.3.1",
    "@zag-js/rating-group": "^1.3.1",
    "@zag-js/slider": "^1.3.1",
    "@zag-js/svelte": "^1.3.1",
    "@zag-js/switch": "^1.3.1",
    "@zag-js/tabs": "^1.3.1",
    "@zag-js/tags-input": "^1.3.1",
    "@zag-js/tooltip": "^1.3.1"
  },
  "peerDependencies": {
    "svelte": "^5.20.0",
    "@skeletonlabs/skeleton": "^3.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.19.0",
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.0.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "jsdom": "^26.0.0",
    "publint": "^0.3.9",
    "svelte": "^5.22.1",
    "svelte-check": "^4.1.5",
    "tailwindcss": "^4.0.9",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vitest": "3.0.8",
    "@skeletonlabs/skeleton": "3.0.0"
  },
  "type": "module",
  "scripts": {
    "build": "svelte-package --input src --output dist",
    "dev": "svelte-package --input src --output dist --watch",
    "test": "vitest run",
    "test:watch": "vitest run --watch",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  }
}